<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Use the title from a page's frontmatter if it has one -->
    <title><%= current_page.data.title || "Set: RELOADED" %></title>
    <%= stylesheet_link_tag "site" %>
    <%= favicon_tag 'images/1.jpg' %>
    <%= javascript_include_tag "jquery-3.3.1.min" %>
    <%= javascript_include_tag "site" %>
    <%= javascript_include_tag "card" %>
    <%= javascript_include_tag "handler" %>
    <%= javascript_include_tag "main" %>
    <%= javascript_include_tag "save" %>
  </head>
  <body>
    <h1>Set: RELOADED</h1>
    <button id = "start-btn" class = "strtbutton" onclick = "eventListener($(this).attr('id'))">Start Game!</button>
    <button id = "load-btn" onclick = "eventListener($(this).attr('id'))">Load Game!</button>
    <h2 style = "display: none;" id = "score">Score: </h2>
    <div id = "tableDiv">
    <%#= yield %>
    </div>

  </body>
</html>
